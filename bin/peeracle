#!/usr/bin/env node
'use strict';

var program = require('commander');

program
  .version('0.0.1');

program
  .command('create-metadata', 'generate a metadata file from a media file')
  .command('start-tracker', 'start a tracker server');

program.parse(process.argv);

if (!process.argv.slice(2).length) {
  program.outputHelp();
}
